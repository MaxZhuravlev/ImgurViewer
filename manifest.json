{
  "manifest_version": 2,

  "name": "Imgur better viewer test",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": ["https://imgur.com/a/*", "https://imgur.com/gallery/*"],
      "css": ["mystyles.css"],
      "js": ["myscript.js"]
    }
  ]
}
